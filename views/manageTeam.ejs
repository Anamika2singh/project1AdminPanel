<%- include ('header.ejs') %>
            <!--begin::Wrapper-->
            <div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">
                <!--begin::Content-->
                <div class="content d-flex flex-column flex-column-fluid" id="kt_content">
                    <!--begin::Subheader-->
                    <div class="subheader py-3 py-lg-8 subheader-transparent" id="kt_subheader">
                        <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
                            <!--begin::Info-->
                            <div class="d-flex align-items-center mr-1">
                                <!--begin::Page Heading-->
                                <div class="d-flex align-items-baseline flex-wrap mr-5">
                                    <!--begin::Page Title-->
                                    <h2 class="d-flex align-items-center text-dark font-weight-bold my-1 mr-3">Manage Team</h2>
                                    <!--end::Page Title-->

                                </div>
                                <!--end::Page Heading-->
                            </div>
                             <div><%=msg%></div>
                            <!--end::Info-->
                            <!--begin::Toolbar-->
                            <div class="d-flex align-items-center flex-wrap">
                                <!--begin::Button-->
                                <a href="/team/getAddTeam" class="btn btn-primary btn-fixed-height font-weight-bold px-2 px-lg-5 mr-2">
                                    <i class="ki ki-plus icon-nm"></i> Add Team
                                </a>
                                <!--end::Button-->


                            </div>
                            <!--end::Toolbar-->
                        </div>
                    </div>
                    <!--end::Subheader-->
                    <!--begin::Entry-->
                    <div class="d-flex flex-column-fluid">
                        <!--begin::Container-->
                        <div class="container-fluid">
                            <div class="card card-custom gutter-b">
									
                                <!--begin::Body-->
                                <div class="card-body py-0">
                                <!--begin::Table-->

                                <div class="my-4">
                                    <form>
                                         <div class="row justify-content-end">
                                             <div class="col-md-4">
                                         <div class="input-icon">
                                             <input type="text" name="search" class="form-control" placeholder="Search..." value="<%=preferences.search%>" id="kt_datatable_search_query" />
                                             <span>
                                                     <i class="flaticon2-search-1 text-muted"></i>
                                                 </span>

                                         </div>

                                          </div>
                                             <div class="col-md-3 my-2 px-2 my-md-0">
                                                 <div class="d-flex align-items-center">
                                                     <select name="sort" class="form-control" id="">
                                                                <option value="">Sort By</option>
                                                                <%if(preferences.sort.createdAt == -1){%>
                                                                <option value="NtoO" selected="selected">Newest To Oldest</option>
                                                                <%}else{%>
                                                                <option value="NtoO">Newest To Oldest</option>
                                                                <%}%>
                                                                <%if(preferences.sort.createdAt == 1){%>
                                                                <option value="OtoN" selected="selected">Oldest To Newest</option>
                                                                 <%}else{%>
                                                                <option value="OtoN">Oldest To Newest</option>
                                                                <%}%>
                                                            </select>
                                                 </div>
                                             </div>
                                             <div class="col-md-3 my-2 px-2 my-md-0">
                                                 <div class="d-flex align-items-center">
                                                     <select class="form-control" name="filter" id="">
                                                        <option value="">Filter By Sports</option>
                                                        <%data1.forEach(function(sport){%>
                                                        <%if(String(sport._id) == String(preferences.filter.sports)){%>
                                                         <option value="<%=sport._id%>" selected="selected"><%=sport.name%></option>
                                                         <%}else{%>
                                                         <option value="<%=sport._id%>"><%=sport.name%></option>
                                                       <%}})%>
                                                     </select>
                                                 </div>
                                             </div>
                                                    <div class="col-md-1">
                                                     <div class="d-flex align-items-center">
                                                         <button type="submit" class="btn btn-primary btn-fixed-height font-weight-bold px-2 px-lg-5 mr-2">Apply</button>
                                                     </div>
                                                 </div>
                                                 <div class="col-md-1">
                                                    <div class="d-flex align-items-center">
                                                        <!--begin::Button-->
                                                        <a href="/team/getTeam?reset=1" class="btn btn-warning btn-fixed-height font-weight-bold px-2 px-lg-5 mr-2">
                                                            Reset
                                                        </a>
                                                        <!--end::Button-->
                                                    </div>
                                                </div>
                                         </div>
                                         </form>
                                         </div>
                                       <!--end::Table-->
                                      </div>
                                 <!--end::Body-->
                             </div>
                             <!--begin::Advance Table Widget 1-->
                            <div class="card card-custom gutter-b">

                                <!--begin::Body-->
                                <div class="card-body py-0">
                                    <!--begin::Table-->
                                    <div class="table-responsive">
                                        <table class="table table-head-custom table-vertical-center" id="kt_advance_table_widget_1">
                                            <thead>
                                                <tr class="text-left">
                                                    <th style="width: 50px; white-space: nowrap;" class="label-inline">
                                                        Sr. No.
                                                    </th>
                                                    <th>Name</th>
                                                    <th>Color Code</th>
                                                    <th>Sports</th>
                                                    <th>Created On</th>
                                                    <th>Updated On</th>
                                                    <th>Status</th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% let n=1;
                                                data2.forEach(function(team){%>
                                                <tr>
                                                    <td>
                                                        <%=n%>
                                                    </td>
                                                    <td>
                                                       <a href="/team/getUpdateTeam/<%=team._id%>"><%=team.name%></a> 
                                                    </td>                                              
                                                    <td style="background-color: <%=team.colorCode%>;color:white">
                                                        <%=team.colorCode%>
                                                    </td>
                                                    <td>
                                                        <%
                                                        team.output.forEach(function(sport){%>
                                                          <%=sport.name%>
                                                            <%})%>
                                                    </td>
                                                   
                                                    <td>
                                                        <%=team.createdAt%>
                                                    </td>
                                                    <td>
                                                        <%=team.updatedAt%>
                                                    </td>
                                                    <td>
                                                        <a href="/team/setAction/<%=team.status%>/<%=team._id%>">
                                                        <%if(team.status == 1){%>
                                                            
                                                           
                                                            <span class="svg-icon svg-icon-danger svg-icon-2x"><!--begin::Svg Icon | path:/var/www/preview.keenthemes.com/metronic/releases/2021-05-14-112058/theme/html/demo1/dist/../src/media/svg/icons/General/Settings#3.svg--><svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
                                                                <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                                                                    <rect x="0" y="0" width="24" height="24"/>
                                                                    <rect fill="#000000" opacity="0.3" x="2" y="6" width="21" height="12" rx="6"/>
                                                                    <circle fill="#000000" cx="17" cy="12" r="4"/>
                                                                </g>
                                                            </svg><!--end::Svg Icon--></span>
                                                           

                                                          <%}else{%>
                                                            

                                                            <span class="svg-icon svg-icon-primary svg-icon-2x"><!--begin::Svg Icon | path:/var/www/preview.keenthemes.com/metronic/releases/2021-05-06-223557/theme/html/demo2/dist/../src/media/svg/icons/Code/Lock-circle.svg--><svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
                                                                <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                                                                    <rect x="0" y="0" width="24" height="24"/>
                                                                    <circle fill="#000000" opacity="0.3" cx="12" cy="12" r="10"/>
                                                                    <path d="M14.5,11 C15.0522847,11 15.5,11.4477153 15.5,12 L15.5,15 C15.5,15.5522847 15.0522847,16 14.5,16 L9.5,16 C8.94771525,16 8.5,15.5522847 8.5,15 L8.5,12 C8.5,11.4477153 8.94771525,11 9.5,11 L9.5,10.5 C9.5,9.11928813 10.6192881,8 12,8 C13.3807119,8 14.5,9.11928813 14.5,10.5 L14.5,11 Z M12,9 C11.1715729,9 10.5,9.67157288 10.5,10.5 L10.5,11 L13.5,11 L13.5,10.5 C13.5,9.67157288 12.8284271,9 12,9 Z" fill="#000000"/>
                                                                </g>
                                                            </svg><!--end::Svg Icon--></span>
                                                         
                                                           <%}%>   																	 
                                                             
                                                        </a>
                                                    </td>	


                                                    <td>
                                                        <a id="deletedGlobal" data-href="/team/deleteTeam/<%=team._id%>" class="btn btn-icon btn-danger mb-1 btn-sm"> <i class="flaticon-delete"></i> </a>
                                                    </td>
                                                </tr>
                                                <%n++;})%>


                                            </tbody>
                                        </table>
                                    </div>
                                    <!--end::Table-->
                                </div>
                                <!--end::Body-->
                            </div>
                            <!--end::Advance Table Widget 1-->


                        </div>
                        <!--end::Container-->
                    </div>
                    <!--end::Entry-->
                </div>
                <!--end::Content-->
                <!--begin::Footer-->
                <!--doc: add "bg-white" class to have footer with solod background color-->
                <div class="footer py-4 d-flex flex-lg-column" id="kt_footer">
                    <!--begin::Container-->
                    <div class="container d-flex flex-column flex-md-row align-items-center justify-content-between">
                        <!--begin::Copyright-->
                        <div class="text-dark order-2 order-md-1">
                            <span class="text-muted font-weight-bold mr-2">2021Â©</span>
                            <a href="#" target="_blank" class="text-dark-75 text-hover-primary">Spomo App</a>
                        </div>
                        <!--end::Copyright-->
                    </div>
                    <!--end::Container-->
                </div>
                <!--end::Footer-->
            </div>
            <!--end::Wrapper-->
        </div>
        <!--end::Page-->
    </div>
    <!--end::Main-->


